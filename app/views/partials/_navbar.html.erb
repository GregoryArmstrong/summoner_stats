<% cache cache_key_for("NavBar", "#{current_user}") do %>
<nav>
  <div class="nav-wrapper">
    <a href="/" class="brand-logo center"><h3>SummonerStats</h3></a>
    <ul class="right hide-on-med-and-down" id="nav-mobile">
      <% if current_user %>
        <li><h3><a><%= link_to "Dashboard", user_path(current_user) %></a></h3></li>
        <li><h3><a><%= link_to "Logout", logout_path %></a></h3></li>
      <% end %>
    </ul>
    <ul class="left hide-on-med-and-down" id="nav-mobile">
      <li><h3><a><%= link_to "Free Champions", champions_index_path %></a></h3></li>
      <% if current_user %>
        <li><h3><a><%= link_to "Master League Players", user_master_league_index_path(user_id: current_user.id) %></a></h3></li>
      <% end %>
    </ul>
  </div>
</nav>
<% end %>
