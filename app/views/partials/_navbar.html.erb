<% cache cache_key_for("NavBar", "#{current_user}") do %>
<ul id="dropdown1" class="left dropdown-content match-navbar">
  <li>
    <h4>
      <a>
        <%= link_to "Free Champions", champions_index_path %>
      </a>
    </h4>
  </li>
  <li class="divider"></li>
  <% if current_user %>
    <li>
      <h4>
        <a>
          <%= link_to "Dashboard", user_path(current_user) %>
        </a>
      </h4>
    </li>
    <li class="divider"></li>
    <li>
      <h4>
        <a>
          <%= link_to "Master League Players", user_master_league_index_path(user_id: current_user.id) %>
        </a>
      </h4>
    </li>
    <li class="divider"></li>
    <li>
      <h4>
        <a>
          <%= link_to "Logout", logout_path %>
        </a>
      </h4>
    </li>
  <% end %>
</ul>
<nav>
  <div class="nav-wrapper">
    <a href="/" class="brand-logo center"><h3>SummonerStats</h3></a>
    <ul class="right hide-on-med-and-down">
      <li><a class="dropdown-button" href="#!" data-activates="dropdown1">Menu</a></li>
    </ul>
  </div>
</nav>
<% end %>
